    "test": "dotenv -e .env.test npm run prisma:migrate:deploy & dotenv -e .env.test jest -- -i",
    "prisma:migrate:dev": "npm run dev:load-envs prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy"
  },
